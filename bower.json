{
    "name": "chosen-decorator",
    "version": "0.2",
    "authors": [
        "Florian Roy"
    ],
    "description": "Chosen decorator with polymer",
    "main": "element/*",
    "ignore": [
        "**/.*",
        "node_modules",
        "bower_components",
        "test",
        "tests"
    ],
    "keywords": ["Polymer", "web-components", "chosen", "html", "select", "selectbox"],
    "dependencies": {
        "polymer": "Polymer/polymer#^1.2.*",
        "ugo-jquery-deps": "https://github.com/display-interactive/ugo-jquery-deps.git",
        "iron-ajax": "git@github.com:PolymerElements/iron-ajax.git#^1.0.*",
        "chosen": "https://github.com/harvesthq/chosen/releases/download/v1.6.2/chosen_v1.6.2.zip"
    },
    "devDependencies": {
        "web-component-tester": "*"
    }
}
