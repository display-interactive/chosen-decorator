{
    "name": "chosen-decorator",
    "version": "0.2",
    "authors": [
        "Florian Roy"
    ],
    "description": "Chosen decorator with polymer",
    "main": "element/*",
    "ignore": [
        "**/.*",
        "node_modules",
        "bower_components",
        "test",
        "tests"
    ],
    "keywords": ["Polymer", "web-components", "chosen", "html", "select", "selectbox"],
    "dependencies": {
        "polymer": "Polymer/polymer#^0.8.0-rc.4",
        "jquery": "latest",
        "chosen": "latest"
    },
    "devDependencies": {
        "web-component-tester": "*"
    }
}